{
  "id": null,
  "uid": "apm-prod-datadog-parity-final",
  "title": "APM â€“ Service Overview (Production)",
  "tags": [
    "apm",
    "otel",
    "micrometer",
    "webflux"
  ],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "templating": {
    "list": [
      {
        "name": "service",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(http_server_requests_seconds_count, service)",
        "multi": false
      },
      {
        "name": "endpoint",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(http_server_requests_seconds_count{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}, uri)",
        "multi": true,
        "includeAll": true
      },
      {
        "name": "status",
        "type": "query",
        "datasource": "Prometheus",
        "query": "label_values(http_server_requests_seconds_count, status)",
        "multi": true,
        "includeAll": true
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Requests / sec",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[1m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2
        }
      }
    },
    {
      "type": "stat",
      "title": "Errors / sec",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"$service\",status=~\"4..|5..\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[1m]))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2
        }
      }
    },
    {
      "type": "stat",
      "title": "Error Rate %",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "targets": [
        {
          "expr": "(sum(rate(http_server_requests_seconds_count{service=\"$service\",status=~\"4..|5..\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[1m])) / sum(rate(http_server_requests_seconds_count{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[1m]))) * 100"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2
        }
      }
    },
    {
      "type": "stat",
      "title": "Total Requests",
      "targets": [
        {
          "expr": "sum(http_server_requests_seconds_count{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"})"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "center",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        }
      },
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      }
    },
    {
      "type": "stat",
      "title": "p50 Latency (ms)",
      "targets": [
        {
          "expr": "clamp_min(histogram_quantile(0.50, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 6,
        "h": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      }
    },
    {
      "type": "stat",
      "title": "p75 Latency (ms)",
      "targets": [
        {
          "expr": "clamp_min(histogram_quantile(0.75, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        }
      ],
      "gridPos": {
        "x": 6,
        "y": 4,
        "w": 6,
        "h": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      }
    },
    {
      "type": "stat",
      "title": "p90 Latency (ms)",
      "targets": [
        {
          "expr": "clamp_min(histogram_quantile(0.90, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 6,
        "h": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      }
    },
    {
      "type": "stat",
      "title": "p95 Latency (ms)",
      "targets": [
        {
          "expr": "clamp_min(histogram_quantile(0.95, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        }
      ],
      "gridPos": {
        "x": 18,
        "y": 4,
        "w": 6,
        "h": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Latency (p50 / p75 / p90 / p95)",
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 24,
        "h": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.5, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.75, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000",
          "legendFormat": "p75"
        },
        {
          "expr": "histogram_quantile(0.9, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000",
          "legendFormat": "p90"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      }
    },
    {
      "type": "table",
      "title": "Endpoint Latency (ms)",
      "targets": [
        {
          "refId": "RPS",
          "expr": "sum by (uri)(rate(http_server_requests_seconds_count{service=\"$service\",uri=~\"$endpoint\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))"
        },
        {
          "refId": "P50",
          "expr": "clamp_min(histogram_quantile(0.5, sum by (le, uri)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri=~\"$endpoint\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        },
        {
          "refId": "P75",
          "expr": "clamp_min(histogram_quantile(0.75, sum by (le, uri)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri=~\"$endpoint\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        },
        {
          "refId": "P90",
          "expr": "clamp_min(histogram_quantile(0.90, sum by (le, uri)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri=~\"$endpoint\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        },
        {
          "refId": "P95",
          "expr": "clamp_min(histogram_quantile(0.95, sum by (le, uri)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri=~\"$endpoint\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0)"
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields"
        },
        {
          "id": "merge"
        },
        {
          "id": "organize",
          "options": {
            "renameByName": {
              "Value #RPS": "Requests / sec",
              "Value #P50": "p50 latency",
              "Value #P75": "p75 latency",
              "Value #P90": "p90 latency",
              "Value #P95": "p95 latency"
            },
            "order": [
              "uri",
              "Requests / sec",
              "p50 latency",
              "p75 latency",
              "p90 latency",
              "p95 latency"
            ]
          }
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 14,
        "w": 24,
        "h": 6
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Requests / sec"
            },
            "properties": [
              {
                "id": "unit",
                "value": "reqps"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "p95 latency",
            "desc": true
          }
        ]
      }
    },
    {
      "type": "table",
      "title": "Slow Endpoints (p95 > 1s)",
      "targets": [
        {
          "expr": "clamp_min(histogram_quantile(0.95, sum by (le, uri)(rate(http_server_requests_seconds_bucket{service=\"$service\",uri=~\"$endpoint\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))) * 1000, 0) > 1000"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 24,
        "h": 6
      }
    },
    {
      "type": "table",
      "title": "Top Errors (4xx / 5xx)",
      "targets": [
        {
          "expr": "sum by (uri, status)(rate(http_server_requests_seconds_count{service=\"$service\",status=~\"4..|5..\",uri!~\"/\\\\*\\\\*|/error|/actuator.*|UNKNOWN\"}[5m]))"
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields"
        },
        {
          "id": "merge"
        },
        {
          "id": "organize",
          "options": {
            "order": [
              "uri",
              "status",
              "Value"
            ],
            "renameByName": {
              "Value": "Errors/sec"
            }
          }
        }
      ],
      "options": {
        "showHeader": true
      },
      "gridPos": {
        "x": 0,
        "y": 26,
        "w": 24,
        "h": 7
      }
    },
    {
      "type": "timeseries",
      "title": "JVM Heap Used",
      "targets": [
        {
          "expr": "jvm_memory_used_bytes{area=\"heap\",service=\"$service\"}",
          "legendFormat": "heap"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 33,
        "w": 12,
        "h": 6
      }
    },
    {
      "type": "timeseries",
      "title": "CPU Usage",
      "targets": [
        {
          "expr": "process_cpu_usage{service=\"$service\"}",
          "legendFormat": "cpu"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 39,
        "w": 12,
        "h": 6
      }
    },
    {
      "type": "timeseries",
      "title": "GC Pause Time",
      "targets": [
        {
          "expr": "rate(jvm_gc_pause_seconds_sum{service=\"$service\"}[1m])",
          "legendFormat": "gc"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 45,
        "w": 24,
        "h": 6
      }
    },
    {
      "type": "timeseries",
      "title": "Kafka Consumer Lag",
      "gridPos": {
        "x": 0,
        "y": 51,
        "w": 24,
        "h": 6
      },
      "targets": [
        {
          "expr": "kafka_consumer_lag{service=\"$service\"}",
          "legendFormat": "{{group}} / {{topic}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "MongoDB Slow Queries (p95)",
      "gridPos": {
        "x": 0,
        "y": 57,
        "w": 24,
        "h": 6
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le, command)(rate(mongodb_driver_commands_seconds_bucket{service=\"$service\"}[5m]))) * 1000",
          "legendFormat": "{{command}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        }
      }
    }
  ]
}